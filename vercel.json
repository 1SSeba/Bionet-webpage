{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*.astro",
        "use": "@astro/build"
      },
      {
        "src": "src/api/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/api/$1.ts"
      }
    ]
  }
  